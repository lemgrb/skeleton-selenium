<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regression Suite" verbose="1" >
    <parameter name="url" value="https://www.duckduckgo.com" />
    <test name="Can search in duckduckgo in Firefox">
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="com.lemgrb.DuckDuckGoSearchTest">
            </class>
        </classes>
    </test>
    <test name="Can search in duckduckgo in Chrome">
        <parameter name="browser" value="Chrome" />
        <classes>
            <class name="com.lemgrb.DuckDuckGoSearchTest">
            </class>
        </classes>
    </test>
</suite>